    <script>

        function ModifyExternalLinks(linksToReplace, mode) {
            var links = jQuery.parseJSON(linksToReplace);
            for (var l in links) {
                var urlObject = links[l];

                var link = urlObject.Url;
                var targetUrl = urlObject.TargetUrl;
                var isValid = urlObject.IsValid;
                var config = urlObject.WatchdogConfig;

                var aElement = $('a[href="' + link + '"]');
                aElement.each(function (index) {

                    var childLength = $(this).children().length;
                    var childrenWithClass = $(this).children('*[class]').length;
                    var isValidChild = true;
                    if (childLength == 0 || $(this).find('img').length == 1 || childrenWithClass == 0) {
                        isValidChild = true;
                    } else {
                        isValidChild = false;
                    }
                    if (isValidChild == false) {
                        $(this).first().contents().unwrap();
                    } else if (isValid == false) {
                        $(this).remove();
                    } else if (mode === "watchdog") {

                        targetUrl = decodeURIComponent(targetUrl);

                        if ($(this).text().substr(0, 4) == "http" && $(this).text() != targetUrl) {
                            targetUrl = $(this).text();
                        }

                        targetUrl = encodeURIComponent(targetUrl);

                        var command = targetUrl + config;
                        $(this).attr("onclick", "msgToWD('" + command + "')");
                        $(this).attr("href", "javascript:void(0);");
                    } else if (mode === "preview") {
                        $(this).attr("href", decodeURIComponent(targetUrl));
                        $(this).attr("target", '_blank');
                    } else if (mode === "edit") {
                        $(this).attr("href", "javascript:void(0)");
                    }
                });

            }
        }
        function openonlinewindow(sURL, sTitle) {
